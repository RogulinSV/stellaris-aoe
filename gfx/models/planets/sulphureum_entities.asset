### Uninhabitable Planets

entity = {
    name = "sulphureum_uninhabitable_planet_01_entity"
    pdxmesh = "planet_clouded_mesh"

    meshsettings = {
        name = "planet_geosphereShape"
        texture_diffuse = "sulphureum_05_diffuse.dds"
        texture_normal = "default_01_normal.dds"
        texture_specular = "default_01_specular.dds"
    }

    ##cloud texture override
    meshsettings = {
        name = "clouds_geosphereShape"
        texture_diffuse = "sulphureum_clouds_01_diffuse.dds"
        texture_normal = "sulphureum_clouds_01_normal.dds"
        texture_specular = "clouds_thin_specular.dds"
    }

    ##cloud shadow texture override
    meshsettings = {
        name = "clouds_shadow_geosphereShape"
        texture_diffuse = "sulphureum_clouds_shadow_01_diffuse.dds"
        texture_normal = "nonormal.dds"
        texture_specular = "nospec.dds"
    }

    default_state = "idle"
    state = { name = "idle" animation = "idle" time_offset = { 0 100 }
        start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class" } }
    }
}

