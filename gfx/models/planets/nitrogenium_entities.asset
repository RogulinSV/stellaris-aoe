### Habitable Planets

entity = {
    name = "nitrogenium_planet_01_entity"
    pdxmesh = "planet_clouded_mesh"

    ##pole texture override
    meshsettings = {
        name = "polesShape"
        texture_diffuse = "poles_1_diffuse.dds"
        texture_normal = "poles_1_normal.dds"
        texture_specular = "poles_1_specular.dds"
    }

    meshsettings = {
        name = "planet_geosphereShape"
        texture_diffuse = "nitrogenium_01_diffuse.dds"
        texture_normal = "continental_02_normals.dds"
        texture_specular = "continental_02_spec.dds"
    }

    ##cloud texture override
    meshsettings = {
        name = "clouds_geosphereShape"
        texture_diffuse = "nitrogenium_clouds_01_diffuse.dds"
        texture_normal = "nitrogenium_clouds_01_normal.dds"
        texture_specular = "clouds_thin_specular.dds"
