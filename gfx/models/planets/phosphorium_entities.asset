### Uninhabitable Planets

entity = {
    name = "phosphorium_uninhabitable_planet_01_entity"
    pdxmesh = "planet_clouded_mesh"

    meshsettings = {
        name = "planet_geosphereShape"
        texture_diffuse = "phosphorium_05_diffuse.dds"
        texture_normal = "phosphorium_05_normal.dds"
        texture_specular = "phosphorium_05_specular.dds"
    }

    ##cloud texture override
    meshsettings = {
        name = "clouds_geosphereShape"
        texture_diffuse = "phosphorium_clouds_01_diffuse.dds"
        texture_normal = "phosphorium_clouds_01_normal.dds"
        texture_specular = "clouds_medium_specular.dds"
    }

    ##cloud shadow texture override
    meshsettings = {
        name = "clouds_shadow_geosphereShape"
        texture_diffuse = "phosphorium_clouds_shadow_02_diffuse.dds"
        texture_normal = "nonormal.dds"
        texture_specular = "nospec.dds"
    }

    default_state = "idle"
    state = { name = "idle" animation = "idle" time_offset = { 0 100 }
        start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class" } }
    }
}

