### Uninhabitable Planets

entity = {
    name = "barium_uninhabitable_planet_01_entity"
    pdxmesh = "planet_clouded_mesh"

    ##pole texture override
    meshsettings = {
        name = "polesShape"
        texture_diffuse = "poles_1_diffuse.dds"
        texture_normal = "poles_1_normal.dds"
        texture_specular = "poles_1_specular.dds"
    }

    meshsettings = {
        name = "planet_geosphereShape"
        texture_diffuse = "barium_05_diffuse.dds"
        texture_normal = "barium_05_normal.dds"
        texture_specular = "barium_05_specular.dds"
    }

    ##cloud texture override
    meshsettings = {
        name = "clouds_geosphereShape"
        texture_diffuse = "barium_clouds_01_diffuse.dds"
        texture_normal = "barium_clouds_01_normal.dds"
        texture_specular = "clouds_thin_specular.dds"
    }

    ##cloud shadow texture override
    meshsettings = {
        name = "clouds_shadow_geosphereShape"
        texture_diffuse = "barium_clouds_shadow_01_diffuse.dds"
        texture_normal = "nonormal.dds"
        texture_specular = "nospec.dds"
    }

    default_state = "idle"
    state = { name = "idle" animation = "idle" time_offset = { 0 100 }
        start_event = { trigger_once = yes sound = { soundeffect = "amb_toxic_world_a" } }
        start_event = { trigger_once = yes sound = { soundeffect = "amb_toxic_world_b" } }
    }
}

